{
  "name": "electron",
  "version": "1.0.113",
  "description": "transcode",
  "main": "index.js",
  "asar": "false",
  "port": 9000,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "clean:d": "rmdir /s /q dist",
    "clean:n": "rmdir /s /q node_modules",
    "build:w": "npm version patch && electron-builder --win --x64",
    "build:m": "npm version patch && electron-builder --mac --x64"
  },
  "author": "lt",
  "license": "ISC",
  "devDependencies": {
    "electron": "27.0.3",
    "electron-builder": "24.6.4"
  },
  "build": {
    "appId": "v1.0.0-transcode",
    "productName": "transcode",
    "extraResources": [
      {
        "from": "./build/ffmpeg.exe",
        "to": "./build/ffmpeg.exe"
      }
    ],
    "asarUnpack": [
      "./ffmpeg.exe"
    ],
    "directories": {
      "output": "dist"
    },
    "files": [
      "index.js",
      "index.html",
      "./src/transfer.js",
      "./build/ffmpeg.exe"
    ],
    "win": {
      "target": "nsis",
      "icon": "./build/wdz.png"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": false,
      "oneClick": false,
      "language": "1033",
      "perMachine": true,
      "createDesktopShortcut": "always"
    }
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "file-type": "^18.7.0",
    "fluent-ffmpeg": "^2.1.2",
    "log4js": "^6.9.1",
    "merge-descriptors": "^1.0.1",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "net": "^1.0.2",
    "on-finished": "^2.4.1",
    "os": "^0.1.2",
    "stream": "^0.0.2"
  }
}
